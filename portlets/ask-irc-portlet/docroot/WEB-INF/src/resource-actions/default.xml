<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE resource-action-mapping PUBLIC "-//Liferay//DTD Resource Action Mapping 6.2.0//EN" "http://www.liferay.com/dtd/liferay-resource-action-mapping_6_2_0.dtd" >
<resource-action-mapping>
  <portlet-resource>
    <portlet-name>ask-irc</portlet-name>
    <permissions>
      <supports>
      	<action-key>ADD_ENTRY</action-key>
		<action-key>MANAGE</action-key>
		<action-key>CONFIGURATION</action-key>
		<action-key>VIEW</action-key>
      </supports>
      <community-defaults>
        <action-key>ADD_ENTRY</action-key>
        <action-key>VIEW</action-key>
      </community-defaults>
      <guest-defaults>
        <action-key></action-key>
      </guest-defaults>
      <guest-unsupported>
        <action-key>MANAGE</action-key>
        <action-key>CONFIGURATION</action-key>
      </guest-unsupported>
      <layout-manager>
      	<action-key>ADD_ENTRY</action-key>
		<action-key>MANAGE</action-key>
		<action-key>CONFIGURATION</action-key>
		<action-key>VIEW</action-key>
      </layout-manager>
      <owner-defaults>
        <action-key>ADD_ENTRY</action-key>
		<action-key>MANAGE</action-key>
		<action-key>CONFIGURATION</action-key>
		<action-key>VIEW</action-key>
      </owner-defaults>
    </permissions>
  </portlet-resource>
</resource-action-mapping>
